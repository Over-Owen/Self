        android:supportsRtl="true"
        android:theme="@style/Theme.Recycler_view_test">
        <activity android:name=".recyclerview_test_ver.RecyclerViewVerActivity"></activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".recyclerview_test.RecyclerViewActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>